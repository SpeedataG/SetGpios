<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#80000000"
    tools:ignore="MissingDefaultResource">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <LinearLayout
            android:layout_width="195dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="0dp"
            android:background="@color/white"
            android:gravity="start"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">


            <TextView
                android:id="@+id/balance_title"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:layout_marginStart="0dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:gravity="center"
                android:text="@string/type_list"
                android:textColor="@color/black_menu_title"
                android:textSize="18sp"
                android:visibility="gone"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view_end_line_title"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color" />

            <TextView
                android:id="@+id/balance_one"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:gravity="start|center"
                android:text="MAIN"
                android:textColor="@color/color_tab_choose_un"
                android:textSize="16sp"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view_end_line1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color" />

            <TextView
                android:id="@+id/balance_two"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:gravity="start|center"
                android:text="EXPAND"
                android:textColor="@color/color_tab_choose_un"
                android:textSize="16sp"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view_end_line2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color" />


            <TextView
                android:id="@+id/balance_three"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:gravity="start|center"
                android:text="EXPAND2"
                android:textColor="@color/color_tab_choose_un"
                android:textSize="16sp"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view_end_line3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color" />

            <TextView
                android:id="@+id/balance_four"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:gravity="start|center"
                android:text="NEW_MAIN"
                android:textColor="@color/color_tab_choose_un"
                android:textSize="16sp"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view_end_line4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color" />

            <TextView
                android:id="@+id/balance_five"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:gravity="start|center"
                android:text="NEW_MAIN_FG"
                android:textColor="@color/color_tab_choose_un"
                android:textSize="16sp"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view_end_line5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color" />

            <TextView
                android:id="@+id/balance_six"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:gravity="start|center"
                android:text="NEW_MAIN_SC"
                android:textColor="@color/color_tab_choose_un"
                android:textSize="16sp"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view_end_line6"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color" />

            <TextView
                android:id="@+id/balance_seven"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:gravity="start|center"
                android:text="MAIN_ZR"
                android:textColor="@color/color_tab_choose_un"
                android:textSize="16sp"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view_end_line7"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color" />

            <TextView
                android:id="@+id/balance_eight"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:gravity="start|center"
                android:text="MAIN_RK"
                android:textColor="@color/color_tab_choose_un"
                android:textSize="16sp"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view_end_line8"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color" />

            <View
                android:id="@+id/bottom_space"
                android:layout_width="match_parent"
                android:layout_height="100dp" />

        </LinearLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>

